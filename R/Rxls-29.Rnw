do.calc <- function(x) {
    pb <- progress(sprintf(" [%%d/%d]", length(x)), 0, lazyness = 0.3)
    on.exit({
        pb$refresh()
        pb$rm()
    })
    sapply(x, function(y) {
               pb$inc()
               ## egy számolási lépés y-on
           })
}
